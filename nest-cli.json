      "entryFile": "main",
      "sourceRoot": "apps/ship/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ship/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "traffic-control": {
      "type": "application",
