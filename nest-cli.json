    "messaging": {
      "type": "application",
      "root": "apps/messaging",
      "entryFile": "main",
      "sourceRoot": "apps/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "apps/messaging/tsconfig.app.json"
      }
    },
    "refilling": {
      "type": "application",
